@model IEnumerable<Asn3.Models.Question>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Qname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qdatetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categoryid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Catobjref)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Qname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qdatetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Categoryid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Catobjref)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Qid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Qid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Qid })
        </td>
    </tr>
}

</table>
