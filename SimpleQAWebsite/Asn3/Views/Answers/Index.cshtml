@model IEnumerable<Asn3.Models.Answer>

@{
    ViewBag.Title = "Index";
}

<h2>Answers</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AnswerText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnswerDateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AnswerText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnswerDateTime)
        </td>
    </tr>
}

</table>
